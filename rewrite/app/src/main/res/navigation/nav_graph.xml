<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainActivity">

    <fragment
        android:id="@+id/planFragment"
        android:name="de.ironjan.metalonly.PlanFragment"
        android:label="fragment_plan"
        tools:layout="@layout/fragment_plan" />
    <activity
        android:id="@+id/mainActivity"
        android:name="de.ironjan.metalonly.MainActivity"
        android:label="content_main"
        tools:layout="@layout/content_main" >
        <action
            android:id="@+id/action_mainActivity3_to_planFragment"
            app:destination="@id/planFragment" />
        <action
            android:id="@+id/action_mainActivity3_to_wishFragment"
            app:destination="@id/wishFragment" />
    </activity>
    <fragment
        android:id="@+id/wishFragment"
        android:name="de.ironjan.metalonly.WishFragment"
        android:label="fragment_wish"
        tools:layout="@layout/fragment_wish" />
    <fragment
        android:id="@+id/streamControlFragment2"
        android:name="de.ironjan.metalonly.StreamControlFragment"
        android:label="fragment_stream_control"
        tools:layout="@layout/fragment_stream" />
</navigation>